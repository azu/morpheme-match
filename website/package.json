{
  "private": true,
  "version": "2.0.4",
  "name": "website",
  "files": [
    "src/",
    "lib/"
  ],
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test": "mocha \"test/**/*.js\"",
    "copy-dict:dist": "mkdir -p dist/dict && cp -r ../node_modules/kuromoji/dict/* dist/dict",
    "clean": "rimraf dist",
    "prestart": "npm-run-all -s clean copy-dict:dist",
    "start": "parcel public/index.html",
    "prebuild": "npm-run-all -s clean copy-dict:dist",
    "build": "parcel build public/index.html --public-url ./",
    "update:sw": "sw-precache --config=sw-precache-config.json && mv service-worker.js public"
  },
  "keywords": [
    "morpheme",
    "形態素解析",
    "japanese",
    "text"
  ],
  "description": "",
  "devDependencies": {
    "@babel/core": "^7.4.5",
    "@babel/register": "^7.13.16",
    "mocha": "^8.3.2",
    "npm-run-all": "^4.1.5",
    "parcel-bundler": "^1.12.3",
    "power-assert": "^1.4.1",
    "rimraf": "^3.0.2",
    "sw-precache": "^4.0.0"
  },
  "dependencies": {
    "almin": "^0.19.0",
    "almin-logger": "^6.2.4",
    "bulma": "0.0.28",
    "classname": "0.0.0",
    "history": "^3.0.0",
    "kuromojin": "^3.0.0",
    "map-like": "^2.0.0",
    "morpheme-match": "^2.0.4",
    "prop-types": "^15.7.2",
    "query-string": "^4.2.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-github-corner": "^0.3.0",
    "react-prop-types": "^0.4.0",
    "usertiming": "^0.1.8"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}

{
  "private": true,
  "version": "1.2.1",
  "name": "website",
  "files": [
    "src/",
    "lib/"
  ],
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test": "mocha test/",
    "copy-dict": "mkdir -p dist/dict && cp -r public/dict/* dist/dict",
    "clean": "rimraf dist",
    "prestart": "npm-run-all -s clean copy-dict",
    "start": "parcel public/index.html",
    "prebuild": "npm-run-all -s clean copy-dict",
    "build": "parcel build public/index.html --public-url ./",
    "update:sw": "sw-precache --config=sw-precache-config.json && mv service-worker.js public"
  },
  "keywords": [
    "morpheme",
    "形態素解析",
    "japanese",
    "text"
  ],
  "description": "",
  "devDependencies": {
    "@babel/core": "^7.4.5",
    "mocha": "^2.5.3",
    "npm-run-all": "^1.7.0",
    "parcel-bundler": "^1.12.3",
    "power-assert": "^1.4.1",
    "rimraf": "^2.5.2",
    "sw-precache": "^4.0.0"
  },
  "dependencies": {
    "almin": "^0.4.4",
    "almin-logger": "^1.1.3",
    "bulma": "0.0.28",
    "classname": "0.0.0",
    "history": "^3.0.0",
    "kuromojin": "^1.3.0",
    "map-like": "^1.0.1",
    "morpheme-match": "^1.2.1",
    "query-string": "^4.2.2",
    "react": "^15.1.0",
    "react-dom": "^15.1.0",
    "react-github-corner": "^0.3.0",
    "usertiming": "^0.1.8"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
